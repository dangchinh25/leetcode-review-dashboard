{
  "name": "leetcode-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "temp-database-up": "docker-compose -f docker-compose.dev.yml up -d",
    "temp-database-down": "docker-compose down",
    "temp-database-migrate": "npx prisma migrate dev",
    "temp-database-seed": "npx prisma db seed",
    "lint": "next lint",
    "lint:eslint": "DEBUG=eslint:cli-engine pnpm eslint --fix --ext .ts,.tsx,.js,.jsx src",
    "lint:eslint:check": "pnpm eslint --ext .ts,.tsx,.js,.jsx src",
    "lint:prettier": "pnpm prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:prettier:check": "pnpm prettier --check 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix:all": "pnpm lint:eslint && pnpm lint:prettier"
  },
  "prisma": {
    "seed": "ts-node src/backend/seed/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "6.3.0",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-table": "^8.20.6",
    "@trpc/client": "11.0.0-rc.730",
    "@trpc/next": "11.0.0-rc.730",
    "@trpc/react-query": "11.0.0-rc.730",
    "@trpc/server": "11.0.0-rc.730",
    "leetcode-query": "^1.2.3",
    "next": "15.1.6",
    "prisma": "^6.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ts-node": "^10.9.2",
    "zod": "^3.24.1",
    "zod-prisma-types": "^3.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.19.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "8.57.0",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5"
  }
}